<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Library</OutputType>
    <TargetFramework>net9.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
    <AssemblyVersion>0.0.2</AssemblyVersion>
    <FileVersion>0.0.2</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\Core\Core.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Forms\AddEventFile.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="Forms\AddFromList.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="Forms\ValuesEditorForm.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="Forms\TagsEditorForm.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="Forms\UsageEditorForm.cs" />
  </ItemGroup>

</Project>